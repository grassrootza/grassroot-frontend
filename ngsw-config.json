{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ],
        "urls": [
          "https://fonts.googleapis.com/css?family=Nunito",
          "https://use.fontawesome.com/releases/v5.0.2/js/all.js"
        ]
      }
    }
  ],
  "dataGroups":[
    {
      "name": "groupList",
      "urls": [
        "https://app.grassroot.org.za/v2/api/group/fetch/list"
      ],
      "version": 1,
      "cacheConfig": {
        "maxSize": 5,
        "maxAge": "12h",
        "strategy": "performance"
      }
    },
    {
      "name": "newMembersList",
      "urls": [
        "https://app.grassroot.org.za/v2/api/group/fetch/members/new"
      ],
      "version": 1,
      "cacheConfig": {
        "maxSize": 5,
        "maxAge": "12h",
        "strategy": "performance"
      }
    }
  ]
}
