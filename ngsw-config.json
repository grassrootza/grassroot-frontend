{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ],
        "urls": [
          "https://fonts.googleapis.com/css?family=Nunito",
          "https://cdn.quilljs.com/1.2.2/quill.snow.css",
          "https://use.fontawesome.com/releases/v5.0.2/js/all.js"
        ]
      }
    }
  ],
  "dataGroups":[
    {
      "name": "news",
      "urls": [
        "https://staging.grassroot.org.za/v2/news"
      ],
      "cacheConfig": {
        "maxSize": 5,
        "maxAge": "12h",
        "timeOut": "20s",
        "strategy": "freshness"
      }
    },
    {
      "name": "newsList",
      "urls": [
        "https://staging.grassroot.org.za/v2/api/news/list/full"
      ],
      "queryParameters": [
        "10",
        "creationTime,desc",
        "0"
      ],
      "cacheConfig": {
        "maxSize": 5,
        "maxAge": "12h",
        "timeOut": "20s",
        "strategy": "freshness"
      }
    },
    {
      "name": "newsHeadlines",
      "urls": [
        "https://reqres.in/api/users?page=2"
      ],
      "version": 1,
      "cacheConfig": {
        "maxSize": 5,
        "maxAge": "12h",
        "strategy": "performance"
      }
    },
    {
      "name": "groupList",
      "urls": [
        "http://localhost:3000/posts"
      ],
      "version": 1,
      "cacheConfig": {
        "maxSize": 5,
        "maxAge": "12h",
        "strategy": "performance",
        "timeOut": 5000
      }
    }
  ]
}
